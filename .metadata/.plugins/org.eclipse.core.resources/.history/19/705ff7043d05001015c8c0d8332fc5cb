<%@ page import="java.io.*"%>
<jsp:useBean id="wf" class="com.yong.wf.WebFolderDAO" scope="session"></jsp:useBean>
<%
String path = request.getRealPath(request.getParameter("file"));
System.out.println(path);
File f = new File(path);
wf.deleteFile(f);
%>
<script>
	window.alert('파일 삭제 성공!');
	opener.location.href = 'webfolder.jsp?crpath=<%=wf.getCrpath()%>';
	window.self.close();
</script>